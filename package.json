{
  "name": "contenta_vue_nuxt",
  "version": "2.0.0",
  "description": "Nuxt.js client for Contenta CMS",
  "author": "Contenta CMS (http://contentacms.org)",
  "contributors": [
    {
      "name": "Yann",
      "email": "yann@yineo.fr",
      "url": "https://yineo.fr"
    },
    {
      "name": "Mateu Aguil√≥ Bosch",
      "email": "mateu.aguilo.bosch@gmail.com",
      "url": "http://humanbits.es"
    }
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@emdaer/plugin-documentation": "^3.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jsonapi-parse": "1.2.0",
    "nuxt": "^2.2.0",
    "vue-lazyload": "^1.2.6"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "start": "npm run frontend:start",
    "build": "npm run frontend:build",
    "precommit": "lint-staged && npm run documentation",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepush": "npm run lint && npm test",
    "prepare": "npm run frontend:build",
    "frontend:dev": "node $NODE_DEBUG_OPTION node_modules/.bin/nuxt dev frontend",
    "frontend:build": "node $NODE_DEBUG_OPTION node_modules/.bin/nuxt build frontend",
    "frontend:analyse": "node $NODE_DEBUG_OPTION node_modules/.bin/nuxt build frontend --analyse",
    "frontend:start": "node $NODE_DEBUG_OPTION node_modules/.bin/nuxt start frontend",
    "frontend:generate": "node $NODE_DEBUG_OPTION node_modules/.bin/nuxt generate frontend",
    "test": "npm run build && karma start frontend/test/unit/karma.conf.js",
    "test-ci": "karma start frontend/test/unit/karma.conf.js",
    "documentation": "emdaer --yes && git add *.md",
    "prettier": "prettier --write './**/*.{js,vue}' && eslint './**/*.{js,vue}' --fix",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-angular": "^6.1.3",
    "@emdaer/cli": "^3.2.3",
    "@emdaer/plugin-contributors-details-github": "^3.2.3",
    "@emdaer/plugin-image": "^3.2.3",
    "@emdaer/plugin-import": "^3.2.3",
    "@emdaer/plugin-license-reference": "^3.2.3",
    "@emdaer/plugin-shields": "^3.2.3",
    "@emdaer/plugin-value-from-package": "^3.2.3",
    "@emdaer/transform-prettier": "^3.2.3",
    "@emdaer/transform-table-of-contents": "^3.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "cross-env": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-problems": "^0.1.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "^2.6.4",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^7.1.0",
    "prettier": "^1.14.3",
    "semantic-release": "^15.4.1",
    "semantic-release-conventional-commits": "^2.0.0"
  }
}
